<AnchorPane prefHeight="100.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.View.ConfirmOverviewController">
   <children>
      <Label layoutX="19.0" layoutY="29.0" text="Are you sure? You wont undo it!">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button layoutX="47.0" layoutY="61.0" mnemonicParsing="false" onAction="#handleConfirm" text="Confirm" />
      <Button layoutX="139.0" layoutY="61.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
   </children>
</AnchorPane>
